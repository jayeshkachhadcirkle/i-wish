{% if block.settings.enable_app_script %}
  <script src="{{ 'script.js' | asset_url }}" defer></script>
{% endif %}

{% schema %}
{
  "name": "iWish Embed",
  "target": "head",
  "settings": [
    {
      "type": "checkbox",
      "id": "enable_app_script",
      "label": "Enable app script",
      "default": true
    }
  ]
}
{% endschema %}
